{
  "name": "test-app",
  "type": "module",
  "private": true,
  "dependencies": {
    "@pikku/core": "^0.12.2",
    "@pikku/jose": "^0.12.1",
    "@pikku/schema-cfworker": "^0.12.1",
    "@pikku/templates-function-addon": "workspace:*",
    "eventsource": "^4.1.0",
    "ws": "^8.19.0",
    "zod": "^4",
    "@pikku/aws-services": "^0.12.2",
    "@pikku/lambda": "^0.12.2",
    "@pikku/pg": "^0.12.1",
    "postgres": "^3.4.8",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@pikku/cli": "^0.12.2",
    "@pikku/fetch": "^0.12.0",
    "@pikku/websocket": "^0.12.1",
    "@types/node": "^24",
    "@types/ws": "^8",
    "concurrently": "^9.2.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9",
    "@aws-sdk/client-apigatewaymanagementapi": "^3.1000.0",
    "@aws-sdk/client-s3": "^3.1000.0",
    "@aws-sdk/client-secrets-manager": "^3.1000.0",
    "@aws-sdk/client-sqs": "^3.1000.0",
    "@aws-sdk/cloudfront-signer": "^3.999.0",
    "@aws-sdk/s3-request-presigner": "^3.1000.0",
    "@types/aws-lambda": "^8.10.161",
    "esbuild": "^0.27.3",
    "serverless": "^4.33.0",
    "serverless-esbuild": "^1.57.0",
    "serverless-offline": "^14.5.0"
  },
  "scripts": {
    "build": "npm run build:offline --external:\"@aws-sdk/*\"",
    "ncu:pikku": "npx npm-check-updates -f '/@pikku/.*/'",
    "pikku": "pikku all",
    "test:http-fetch": "npx tsx client/http-fetch.ts",
    "test:http-sse": "npx tsx client/http-sse.ts",
    "test:rpc": "npx tsx client/rpc.ts",
    "test:websocket": "npx tsx client/websocket.ts",
    "test:cli:local": "npx tsx client/cli-local.gen.ts",
    "test:workflows": "npx tsx client/workflow.ts",
    "test:triggers": "npx tsx client/trigger.ts",
    "test:agent-http": "npx tsx client/agent-http.ts",
    "test:agent-sse": "npx tsx client/agent-sse.ts",
    "build:offline": "rm -rf dist && esbuild ./src/websocket.ts --format=esm --minify --bundle --keep-names --sourcemap --platform=node --target=node24 --outdir=dist --out-extension:.js=.mjs --metafile=meta.json --packages=external",
    "start": "npm run build:offline && SLS_TELEMETRY_DISABLED=1 SLS_NOTIFICATIONS_MODE=off serverless offline",
    "dev": "npm run build:offline && serverless offline",
    "test": "npm run test:websocket",
    "deploy": "npm run build && serverless deploy",
    "postinstall": "pikku all",
    "stackblitz": "concurrently 'pikku watch --silent' 'npm run dev'"
  },
  "main": "dist/index.mjs",
  "stackblitz": {
    "startCommand": "npm run stackblitz"
  }
}