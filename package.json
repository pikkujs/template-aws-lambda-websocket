{
  "name": "test-app",
  "type": "module",
  "private": true,
  "dependencies": {
    "@pikku/core": "^0.9.7",
    "@pikku/jose": "^0.9.3",
    "@pikku/schema-cfworker": "^0.9.2",
    "eventsource": "^4.0.0",
    "@pikku/aws-services": "^0.9.2",
    "@pikku/lambda": "^0.9.3",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@pikku/cli": "^0.9.7",
    "@pikku/fetch": "^0.9.2",
    "@pikku/websocket": "^0.9.2",
    "@types/node": "^24",
    "concurrently": "^9.2.0",
    "typescript": "^5.9",
    "@aws-sdk/client-apigatewaymanagementapi": "^3.864.0",
    "@aws-sdk/client-secrets-manager": "^3.864.0",
    "@types/aws-lambda": "^8.10.152",
    "esbuild": "^0.25.9",
    "serverless": "^4.18.0",
    "serverless-esbuild": "^1.55.1",
    "serverless-offline": "^14.4.0",
    "tsx": "^4.20.4"
  },
  "scripts": {
    "pikku": "pikku all",
    "test:template": "bash run-tests.sh --url http://localhost:3000/production --websocket",
    "test:http": "npx tsx client/http-fetch.ts",
    "test:sse": "npx tsx client/http-sse.ts",
    "test:rpc": "npx tsx client/rpc.ts",
    "test:websocket": "npx tsx client/websocket.ts",
    "build:offline": "rm -rf dist && esbuild ./src/websocket.ts --format=esm --minify --bundle --keep-names --sourcemap --platform=node --target=node20 --outdir=dist --out-extension:.js=.mjs --metafile=meta.json",
    "dev": "npm run build:offline && serverless offline",
    "build": "npm run build:offline --external:\"@aws-sdk/*\"",
    "deploy": "npm run build && serverless deploy",
    "postinstall": "pikku all",
    "test": "npm run test:websocket",
    "stackblitz": "concurrently 'pikku --watch --silent' 'npm run dev'"
  },
  "main": "dist/index.mjs",
  "stackblitz": {
    "startCommand": "npm run stackblitz"
  }
}